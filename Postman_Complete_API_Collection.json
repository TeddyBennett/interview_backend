			"description": "Endpoints for Administrator authentication and API Key lifecycle management.",
			"item": [
				{
					"name": "Admin Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = pm.response.json();",
									"if (pm.response.code === 200 && jsonData.data && jsonData.data.token) {",
									"    pm.environment.set(\"adminJwtToken\", jsonData.data.token);",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"username\": \"admin\",\n    \"password\": \"admin123\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseUrl}}/api/admin/login",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"api",
								"admin",
								"login"
							]
						}
					}
				},
